            <button type="button" class="btn btn-sm btn-outline-info" id="blind-ratings">Toggle Blind Ratings</button>
            <button type="button" class="btn btn-sm btn-outline-secondary" data-bs-dismiss="modal">Skip Score</button>
            <button type="button" class="btn btn-sm btn-outline-success" id="add-chat-score">Enable Chat Score</button>
            <button type="button" class="btn btn-sm btn-outline-primary" id="submitRating">Final Judgement</button>
          </div> <!-- End modal-footer -->
        </div> <!-- End modal-body -->
      </div> <!-- End modal-content -->
    </div> <!-- End modal-dialog -->
  </div> <!-- End modal -->
  <div class="modal" id="rating-modal" class="rating-modal" tabindex="-1" role="dialog" aria-labelledby="newRatingModalLabel" aria-hidden="true" style="z-index: 1200;">
    <div class="modal-dialog" role="document">
      <div class="modal-content rating-modal-content">
        <div class="modal-header">
          <h3 id="rating-username"><span></span></h3>
        </div>
        <div class="modal-body">
          <h1 class="modal-title" id="newRatingModalLabel"><span class="this-rating"></span> <i class="fas fa-star"></i>'s</h1>
          <div class="container d-flex align-items-stretch">
            <div class="row">
              <div class="col-md-6">
                <p id="rating-rank">Ranking:<br /> <span></span></p>
              </div>
              <div class="col-md-6">
                <p id="rating-weighted">Weighted Social Score:<br /> <span></span></p>
              </div>
              <div class="col-md-6">
                <p id="rating-change-rank">Change in Rank:<br /> <span></span></p>
              </div>
              <div class="col-md-6">
                <p id="rating-change-rating">Change in Rating:<br /> <span></span></p>
            </div>
            <hr>
            <div id="rating-top10-changes"></div>
          </div>
        </div>
      </div>
    </div> <!-- End rating-modal-content -->
  </div> <!-- End rating-modal -->
</div> <!-- End modal -->
<div class="modal" id="watchYoutubeModal" tabindex="-1" aria-labelledby="videoModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-fullscreen">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title fs-5" id="youtubeVideoTitle"></h5>
        <button type="button" class="btn btn-outline-success" data-bs-dismiss="modal" aria-label="Close" data-toggle="tooltip" data-placement="top" title="Start Rating">Start Rating</button>
      </div>
      <div class="modal-body" id="watchYoutubeModalBody">
        <br />
        <!-- 16:9 aspect ratio -->
        <div class="container-flex">
          <div class="ratio ratio-16x9">
            <div id="youtube-player"></div>
          </div>
        </div>
      </div>
        <!-- Media controls -->
        <div class="d-flex justify-content-between align-items-center">
          <button type="button" class="btn btn-outline-default" id="youtube-play-pause" aria-label="Left Align">
            <div style="width: 24px; text-align: center;"> <!-- adjust the width as needed -->
              <i class="fas fa-play fa-2x active" aria-hidden="true"></i>
              <i class="fas fa-pause fa-2x inactive" aria-hidden="true" style="display:none;"></i>
            </div>
          </button>
          <div class="progress" style="flex-grow: 1; margin: 0 10px;">
            <div class="progress-bar" id="youtube-player-progress-bar" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
          </div>
          <!-- Time left/duration -->
          <div id="time-left-duration">
            <span id="current-time">00:00</span> / <span id="total-duration">00:00</span>
          </div>
          <!-- Volume control -->
          <div class="volume-slider" style="margin-left: 10px;">
            <input type="range" id="volume-control" min="0" max="100" step="1" value="100">
          </div>
          <!-- Closed Captioning -->
          <button type="button" class="btn btn-outline-default" id="youtube-cc" aria-label="Left Align">
            <i class="fas fa-closed-captioning fa-2x active" aria-hidden="true"></i>
          </button>          
        </div>
    </div>
  </div>
</div>
<footer class="footer">
  <div class="d-flex justify-content-center">
    <form class="form-inline">
      <div class="btn-group">
        <button class="btn btn-outline-primary my-2 my-sm-0 shuffle" id="" type="button" data-toggle="tooltip" data-bs-trigger="hover focus" data-placement="top" title="Fisher Yates Randomization">
          <i class="fas fa-random"></i> Shuffle
        </button>
        <button class="btn btn-outline-secondary my-2 my-sm-0" id="unwatched-users" type="button" data-toggle="tooltip" data-placement="top" title="Users without videos watched during this stream first">
          <i class="fas fa-eye-slash"></i> (Really Fixed) Unwatched Users
        </button>
        <button class="btn btn-outline-info my-2 my-sm-0" id="time-sort" type="button" data-toggle="tooltip" data-placement="top" title="Shortest videos first">
          <i class="fas fa-clock"></i> Duration Short
        </button>
        <button class="btn btn-outline-warning my-2 my-sm-0" id="make_fair" type="button" data-toggle="tooltip" data-placement="top" title="Users with fewest ratings per season shown first">
          <i class="fas fa-balance-scale"></i> Make Fair
        </button>
        <button class="btn btn-outline-success my-2 my-sm-0" id="social_sort" type="button" data-toggle="tooltip" data-placement="top" title="Highest social scores first">
          <i class="fas fa-sort-amount-up"></i> Social Sort
        </button>
        <button class="btn btn-outline-light my-2 my-sm-0" id="reopen_modal" type="button" data-toggle="tooltip" data-placement="top" title="Reopen Rating">
          <i class="fas fa-window-restore"></i> Reopen Rating
        </button>
      </div>
    </form>
  </div>
</footer>
        <!-- Footer -->
</body>

</html>