    @keyframes fallOut {
  0% { transform: translateY(0) rotate(0deg); opacity: 1; }
  20% { transform: translateY(20px) rotate(5deg); opacity: 0.8; }
  100% { transform: translateY(1000px) rotate(30deg); opacity: 0; }
}

@keyframes fadeIn {
  0% { opacity: 0; transform: scale(0.8); }
  100% { opacity: 1; transform: scale(1); }
}

@keyframes strikeThrough {
  0% { width: 0; }
  100% { width: 100%; }
}

.dropped-score {
  position: relative;
  animation: fallOut 1.5s ease-in forwards, fadeIn 0.5s ease-out 1.5s forwards;
  color: #888; /* Grey out the text */
}

.dropped-score::after {
  position: absolute;
    top: 10px;
    bottom: 0;
    left: 0;
    right: 0;
    content: "\274c"; /* use the hex value here... */
    font-size: 9em; 
    color: red;
    line-height: 100px;
    text-align: center;
    animation: strikeThrough 0.5s ease-out 2s forwards;
}


